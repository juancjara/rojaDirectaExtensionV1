{
  "name": "ExtensionV1",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get information from roja directa",
  "permissions": [
    "notifications",
    "http://localhost:8085/"
  ],
  "browser_action": {
    "default_popup" : "popup.html",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "background" : {
    "scripts" : ["popup.js"]
  }
}